{
  "expo": {
    "name": "tinder-app",
    "slug": "tinder-app",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.issatoyin.tinder"
    },
    "android": {
      "package": "com.issatoyin.tinder"
    },
    "assetBundlePatterns": ["**/*"],
    "description": "Keep Swiping",
    "githubUrl": "https://github.com/emaitee/TinderClone"
  }
}
